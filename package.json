{
  "name": "yakusu",
  "version": "1.0.0",
  "private": true,
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "ts": "npx tsx --",
    "typecheck": "tsc --noEmit",
    "test": "for f in $(find . -name '*.test.ts' -not -path './node_modules/*' | sort); do echo \"Running $f\"; npx tsx \"$f\" || exit 1; done",
    "verify": "yarn typecheck && yarn test",
    "dev": "yarn ts ./run/dev.ts",
    "web:dev": "next dev web",
    "ios:sim": "bash -lc 'set -e; DEVICE=\"iPhone 16e\"; open -a Simulator; xcrun simctl boot \"$DEVICE\" || true; xcrun simctl bootstatus \"$DEVICE\" -b; pushd ios/Yakusu >/dev/null; xcodebuild -scheme Yakusu -configuration Debug -destination \"platform=iOS Simulator,name=$DEVICE\" -derivedDataPath .derived build; APP=$(pwd)/.derived/Build/Products/Debug-iphonesimulator/Yakusu.app; BUNDLE_ID=$(/usr/libexec/PlistBuddy -c \"Print :CFBundleIdentifier\" \"$APP/Info.plist\"); popd >/dev/null; xcrun simctl uninstall \"$DEVICE\" \"$BUNDLE_ID\" || true; xcrun simctl install \"$DEVICE\" \"$APP\"; xcrun simctl launch \"$DEVICE\" \"$BUNDLE_ID\"'"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.137",
    "@types/dedent": "^0.7.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^18.2.22",
    "@types/seedrandom": "^3.0.8",
    "@types/unzipper": "^0.10.11",
    "@types/xml2js": "^0.4.14",
    "@types/yargs": "^17.0.33",
    "@types/express": "^5.0.3",
    "@types/handlebars": "^4.1.0",
    "@types/lodash": "^4.17.20",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "playwright": "^1.55.1",
    "prettier": "^3.4.2",
    "sst": "^2.41.6",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.886.0",
    "@aws-sdk/client-s3": "^3.886.0",
    "@aws-sdk/client-sqs": "^3.886.0",
    "@aws-sdk/lib-dynamodb": "^3.886.0",
    "@aws-sdk/s3-request-presigner": "^3.886.0",
    "@aws-sdk/util-dynamodb": "^3.886.0",
    "add": "^2.0.6",
    "chalk": "^5.6.0",
    "css-what": "^7.0.0",
    "dedent": "^1.6.0",
    "dotenv": "^17.2.0",
    "esbuild": "^0.25.6",
    "express": "^5.1.0",
    "glob": "^10.3.12",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "next": "^15.0.0",
    "openai": "^5.19.1",
    "seedrandom": "^3.0.5",
    "ulid": "^2.3.0",
    "unified": "^11.0.5",
    "unzipper": "^0.12.3",
    "yargs": "17.7.2",
    "yarn": "^1.22.22",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  }
}